{
  "expo": {
    "name": "VirtualLab",
    "slug": "VirtualLab",
    "version": "1.0.0",
    "owner": "uas_pawm_kelompok16",
    "orientation": "portrait",
    "icon": "./assets/images/icon-fun4eng.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-adaptive-fun4eng.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.fun4eng.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon-splash-screen-fun4eng.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-dev-client"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ecaefaf5-0f82-4786-af73-05dd3cfec8d3"
      }
    }
  }
}
